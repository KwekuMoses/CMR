ata] = useState({})\r\n  const history = useHistory()\r\n\r\n  function getCustomerItem() {\r\n    const url = `https://frebi.willandskill.eu/api/v1/customers/${customerId}/`\r\n    const token = localStorage.getItem(\"WEBB20\")\r\n    fetch(url, {\r\n      headers: {\r\n        \"Content-Type\": \"application/json\",\r\n        \"Authorization\": `Bearer ${token}`\r\n      }\r\n    })\r\n    .then(res => res.json())\r\n    .then(data => setFormData(data))\r\n  }\r\n\r\n  useEffect( () => {\r\n    getCustomerItem()\r\n  },[])\r\n\r\n  function handleOnChange(e) {\r\n    const name = e.target.name\r\n    const value = e.target.value\r\n    const newObj = {...formData, [name]: value}\r\n    setFormData(newObj)\r\n  }\r\n\r\n  function renderInput(name, label, type) {\r\n    return (\r\n      <div>\r\n        <label>{label}</label>\r\n        <input \r\n          type={type || \"text\"} \r\n          name={name}\r\n          value={formData[name] || \"\"}\r\n          onChange={handleOnChange}\r\n        />\r\n      </div>\r\n    )\r\n  }\r\n\r\n  function handleOnSubmit(e) {\r\n    e.preventDefault()\r\n    const url = `https://frebi.willandskill.eu/api/v1/customers/${customerId}/`\r\n    const token = localStorage.getItem(\"WEBB20\")\r\n    fetch(url, {\r\n      method: \"PUT\",\r\n      body: JSON.stringify(formData),\r\n      headers: {\r\n        \"Content-Type\": \"application/json\",\r\n        \"Authorization\": `Bearer ${token}`\r\n      }\r\n    })\r\n    .then(res => res.json())\r\n    .then(() =>{\r\n      client.getCustomerList().then((data) => setCustomerList(data.results));\r\n      history.push(`/homepage`)\r\n    })\r\n    \r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <h1>Update Customer</h1>\r\n      <form onSubmit={handleOnSubmit}>\r\n        {renderInput(\"name\", \"Customer Name\")}\r\n        {renderInput(\"email\", \"Customer Email\", \"email\")}\r\n        {renderInput(\"organisationNr\", \"Organisation Number\")}\r\n        {renderInput(\"paymentTerm\", \"Payment Term\", \"number\")}\r\n        {renderInput(\"phoneNumber\", \"Phone Number\", \"tel\")}\r\n        {renderInput(\"reference\", \"Reference\")}\r\n        {renderInput(\"vatNr\", \"Vat Number\")}\r\n        {renderInput(\"website\", \"Website\", \"url\")}\r\n        <TomatoButton type=\"submit\">Update Customer</TomatoButton>\r\n\r\n      </form>\r\n\r\n    </div>\r\n  )\r\n}\r\n","D:\\programmering\\Nackademin\\REACT\\Lektion-14JSW\\CMR\\cmr\\src\\styles\\ButtonStyled.jsx",[],"D:\\programmering\\Nackademin\\REACT\\Lektion-14JSW\\CMR\\cmr\\src\\client\\client.js",[],"D:\\programmering\\Nackademin\\REACT\\Lektion-14JSW\\CMR\\cmr\\src\\components\\CreateCustomer.jsx",[],"D:\\programmering\\Nackademin\\REACT\\Lektion-14JSW\\CMR\\cmr\\src\\components\\UserInfo.jsx",[],"D:\\programmering\\Nackademin\\REACT\\Lektion-14JSW\\CMR\\cmr\\src\\components\\CustomerList.jsx",[],{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","severity":1,"message":"73","line":24,"column":5,"nodeType":"74","endLine":24,"endColumn":7,"suggestions":"75"},{"ruleId":"72","severity":1,"message":"73","line":29,"column":5,"nodeType":"74","endLine":29,"endColumn":7,"suggestions":"76"},"no-native-reassign",["77"],"no-negated-in-lhs",["78"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'getCustomerItem'. Either include it or remove the dependency array.","ArrayExpression",["79"],["80"],"no-global-assign","no-unsafe-negation",{"desc":"81","fix":"82"},{"desc":"81","fix":"83"},"Update the dependencies array to be: [getCustomerItem]",{"range":"84","text":"85"},{"range":"86","text":"85"},[742,744],"[getCustomerItem]",[943,945]]